<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainParent" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainUIController">
    <children>
        <Label fx:id="mainLabel" alignment="TOP_LEFT" layoutX="14.0" layoutY="25.0" minHeight="-Infinity"
               minWidth="-Infinity" prefHeight="18.0" prefWidth="600.0" text="Hello World" textAlignment="CENTER"
               AnchorPane.bottomAnchor="357.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="25.0"/>

        <MenuBar prefHeight="25.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                 AnchorPane.topAnchor="0.0">
            <menus>
                <Menu id="file" mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="menuItemOpen" mnemonicParsing="false" text="Open"/>
                    </items>
                    <items>
                        <MenuItem fx:id="menuItemClose" mnemonicParsing="false" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>

        <Button layoutY="43.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="66.0" text="Execute"
                fx:id="executeButton"
                AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="43.0"/>
    </children>
</AnchorPane>
